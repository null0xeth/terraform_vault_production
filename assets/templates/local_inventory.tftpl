[vault]
%{ for name, ipv4 in ansible_map.vault_server ~}
${name} ansible_host=${ipv4} ansible_user=null0x ansible_ssh_private_key_file=~/.ssh/ansible_ed25519
%{ endfor ~}

[loadbalancer]
%{ for name, ipv4 in ansible_map.loadbalancer ~}
${name} ansible_host=${ipv4} ansible_user=null0x ansible_ssh_private_key_file=~/.ssh/ansible_ed25519
%{ endfor ~}
